package Operators;

public class ArithmeticOperators 
{
	public static int preIncre(int n)
	{
		return ++n;
	}
	
	public static int preDecre(int n)
	{
		return --n;
	}
	
	public static int postIncre(int n)
	{
		return n++;
	}
	
	public static int postDecre(int n)
	{
		return n--;
	}
	
	public static int add(int n1, int n2)
	{
		return n1+n2;
	}
	
	public static int sub(int n1, int n2)
	{
		return n1-n2;
	}
	
	public static int mul(int n1, int n2)
	{
		return n1*n2;
	}
	
	public static int div(int n1, int n2)
	{
		return n1/n2;
	}
	
	public static int mod(int n1, int n2)
	{
		return n1%n2;
	}
	
}
